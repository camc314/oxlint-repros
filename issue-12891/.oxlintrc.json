{
  "plugins": ["typescript", "unicorn", "oxc", "import", "jsdoc", "promise", "node"],
  "categories": {
    "correctness": "error",
    "suspicious": "error",
    "pedantic": "error",
    "perf": "error",
    "style": "error",
    "restriction": "error",
    "nursery": "error"
  },
  "rules": {},
  "overrides": [
    {
      "files": ["**/*.{ts,vue}"],
      "rules": {
        "eslint/no-restricted-imports": [
          "error",
          {
            "paths": [{ "name": "lodash", "message": "Please use lodash-es instead" }]
          }
        ]
      }
    }
  ]
}
